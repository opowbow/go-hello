{
    "name": "cloud-run-button",
    "env": {
        "BACKGROUND_COLOR": {
            "description": "red",
            "value": "#fefefe",
            "required": false
        },
        "TITLE": {
            "description": "OPtitle",
            "value": "Oprun",
            "required": false
        },
        "APP_SECRET": {
            "generator": "secret"
        },
        "ORDERED_ENV": {
            "description": "control the order env variables are prompted",
            "order": 100,
            "value": "1",
            "required": false
        }
    },
    "options": {
        "allow-unauthenticated": false,
        "memory": "512Mi",
        "cpu": "1",
        "port": 80,
        "http2": false
    },
    "build": {
        "skip": false,
        "buildpacks": {
            "builder": ""
        }
    },
    "hooks": {
        "prebuild": {
            "commands": [
                ""
            ]
        },
        "postbuild": {
            "commands": [
                ""
            ]
        },
        "precreate": {
            "commands": [
                "echo 'test'"
            ]
        },
        "postcreate": {
            "commands": [
                ""
            ]
        }
    }
}
